  <title>All HTML Features</title>
 =20


  <h1>All HTML Features in One Page</h1>

  <div class=3D"grid-container">

    <!-- 1. Button Color Toggle -->
    <section>
      <h2>1. Button Color Toggle</h2>
      <button class=3D"btn">Toggle Color</button>
    </section>

    <!-- 2. Variables & Data Types -->
    <section>
      <h2>2. Variables &amp; Data Types</h2>
      <p>Check the console for variable output.</p>
     =20
    </section>

    <!-- 3. Simple Calculator -->
    <section>
      <h2>3. Simple Calculator</h2>
      <input id=3D"num1" type=3D"number" placeholder=3D"First number">
      <input id=3D"num2" type=3D"number" placeholder=3D"Second number">
      <select id=3D"operator">
        <option>+</option>
        <option>-</option>
        <option>*</option>
        <option>/</option>
      </select>
      <button class=3D"btn">Calculate</button>
      <p id=3D"calcResult"></p>
     =20
    </section>

    <!-- 4. Digital Clock -->
    <section>
      <h2>4. Digital Clock</h2>
      <h3 id=3D"clock">8:09:24 AM</h3>
     =20
    </section>

    <!-- 5. Light Bulb Toggle -->
    <section>
      <h2>5. Light Bulb Toggle</h2>
      <img id=3D"bulb" src=3D"https://www.w3schools.com/js/pic_bulboff.gif"=
>
      <br>
      <button class=3D"btn">Toggle Bulb</button>
     =20
    </section>

    <!-- 6. Show/Hide Password -->
    <section>
      <h2>6. Show/Hide Password</h2>
      <input type=3D"password" id=3D"pwd" placeholder=3D"Enter password">
      <button class=3D"btn">Show/Hide</button>
     =20
    </section>

    <!-- 7. Registration Form -->
    <section>
      <h2>7. Registration Form</h2>
      <form>
        <input type=3D"text" placeholder=3D"Name" required=3D"">
        <input type=3D"email" placeholder=3D"Email" required=3D"">
        <button class=3D"btn" type=3D"submit">Register</button>
      </form>
    </section>

    <!-- 8. Journal App -->
    <section>
      <h2>8. Journal App</h2>
      <textarea id=3D"journal" placeholder=3D"Write something..."></textare=
a>
      <button class=3D"btn">Save</button>
      <p id=3D"savedText"></p>
     =20
    </section>

    <!-- 9. User Preferences -->
    <section>
      <h2>9. User Preferences</h2>
      <label>Select Theme:
        <select>
          <option value=3D"light">Light</option>
          <option value=3D"dark">Dark</option>
        </select>
      </label>
     =20
    </section>

    <!-- 10. Feedback Form -->
    <section>
      <h2>10. Feedback Form</h2>
      <textarea id=3D"feedback" placeholder=3D"Your feedback..."></textarea=
>
      <button class=3D"btn">Submit</button>
      <p id=3D"feedbackMessage"></p>
     =20
    </section>

    <!-- 11. Placeholder -->
    <section>
      <h2>11. Placeholder Section</h2>
      <p>More features coming soon!</p>
    </section>

    <!-- 12. Counter App -->
    <section>
      <h2>12. Counter App</h2>
      <div id=3D"counter">0</div>
      <button class=3D"increase">Increase</button>
      <button class=3D"decrease">Decrease</button>
      <button class=3D"reset">Reset</button>
     =20
    </section>

  </div>


</body></html>
