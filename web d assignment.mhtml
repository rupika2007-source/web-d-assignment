@charset "utf-8";

body { font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif; backgr=
ound: linear-gradient(to right, rgb(255, 236, 210), rgb(252, 182, 159)); ma=
rgin: 0px; padding: 20px; color: rgb(51, 51, 51); }

h1 { text-align: center; color: rgb(44, 62, 80); }

.grid-container { display: grid; grid-template-columns: 1fr 1fr; gap: 20px;=
 max-width: 1200px; margin: auto; }

section { background: rgb(255, 255, 255); padding: 20px; border-radius: 15p=
x; box-shadow: rgba(0, 0, 0, 0.2) 0px 4px 8px; }

h2 { color: rgb(41, 128, 185); }

input, button, select { margin: 5px; padding: 10px; font-size: 16px; border=
-radius: 5px; border: 1px solid rgb(204, 204, 204); }

.btn { cursor: pointer; background-color: rgb(52, 152, 219); color: white; =
border: none; }

.btn:hover { background-color: rgb(41, 128, 185); }

textarea { width: 100%; height: 100px; }

#bulb { width: 50px; height: 100px; }

#counter { font-size: 50px; font-weight: bold; margin: 20px; }

.increase { background-color: green; color: white; }

.decrease { background-color: red; color: white; }

.reset { background-color: gray; color: white; }

.increase:hover, .decrease:hover, .reset:hover { opacity: 0.8; }
